<http://www.FIXME.org/nsmethods> rdf:type owl:Ontology ;
                                 owl:imports <http://www.FIXME.org/nsupper> .


#################################################################
#
#    Classes
#
#################################################################

:nsm_0000000 rdf:type owl:Class ;
    rdfs:label "method"@en ;
    nsu:definition "A means, manner of procedure, or systematic course of actions that have to be performed in order to accomplish a particular goal."@en ;
    rdfs:subClassOf <http://www.FIXME.org/nsupper#nsu_0000001> .

:nsm_0000001 rdf:type owl:Class ;
     rdfs:label "being delivery"@en ;
     rdfs:subClassOf :nsm_0000000 .

:nsm_0000002 rdf:type owl:Class ;
     rdfs:label "calorimetry"@en ;
     rdfs:subClassOf :nsm_0000000 .

KSC_M:000002 rdf:type owl:Class ;
    rdfs:label "gray matter tissue preparation"@en ;
    nsu:definition "Class of methods that are used to prepare the gray matter tissue of interest for application of other methods."@en ;
    nsu:synonym "brain matter tissue preparation"@en ;
    rdfs:subClassOf :nsm_0000000 .

obo:OBI_0302887 rdf:type owl:Class ;
    rdfs:label "staining method"@en ;
    nsu:definition "Class of methods where specific substances are applied to the gray matter tissue, for identification, visualization, and measurement of objects or processes."@en ;
    nsu:synonym "labeling method"@en ;
    nsu:synonym "staining"@en ;
    rdfs:subClassOf :nsm_0000000 .

KSC_M:000004 rdf:type owl:Class ;
    rdfs:label "visualization method"@en ;
    nsu:definition "Class of methods that are used to make visible the objects of interest or physiological processes to the eye of the experimenter, unaided or using specific devices."@en ;
    rdfs:subClassOf :nsm_0000000 .

obo:ERO_0001319 rdf:type owl:Class ;
    rdfs:label "measurement method"@en ;
    nsu:definition "Class of methods that are used to observe and record amount, number, quantity, size, level, extent, or magnitude, resulting in an ordinal, semiquantitative or quantitative value."@en ;
    nsu:synonym "measurement method"@en ;
    nsu:synonym "measuring method"@en ;
    rdfs:subClassOf :nsm_0000000 .

KSC_M:000006 rdf:type owl:Class ;
    rdfs:label "analysis method"@en ;
    nsu:definition "Class of methods that are used to process and interpret the data produced by measurements, as well as to infer patterns or regularities from data."@en ;
    rdfs:subClassOf :nsm_0000000 .

obo:ERO_0000378 rdf:type owl:Class ;
    rdfs:label "surgery"@en ;
    nsu:definition "Class of methods where the experimenter performs incisions into the body, to gain direct access to the tissue of interest."@en ;
    nsu:synonym "surgery"@en ;
    rdfs:subClassOf :nsm_0000000 .

KSC_M:000008 rdf:type owl:Class ;
    rdfs:label "stereotaxic surgery"@en ;
    nsu:definition "Surgery performed according to the spatial (stereotactic) coordinates of the brain and spinal cord in a given species. These coordinates are calculated relative to a set of landmarks: bregma, lambda, and the interaural distance, and from a standard stereotaxic atlas."@en ;
    rdfs:subClassOf obo:ERO_0000378 .

KSC_M:000009 rdf:type owl:Class ;
    rdfs:label "basophilic stain method"@en ;
    nsu:definition "Class of histochemical methods where the dyes label the nucleic acids (RNA)."@en ;
    nsu:synonym "basophilic method"@en ;
    rdfs:subClassOf nifstd:nlx_inv_20090605 .

nifstd:birnlex_2247 rdf:type owl:Class ;
    rdfs:label "nissl method"@en ;
    nsu:definition "Histochemical method for labeling neurons. Application of basophilic dyes, such as Cresyl violet, Thionin and Toluidin Blue, to the brain tissue labels the nuclei of cell bodies of neurons, and of glia. These dyes bind preferentially to the RNA present in the cell, and the cell nuclei will be most stained because of the high presence of this nucleic acid."@en ;
    nsu:synonym "nissl stain"@en ;
    nsu:synonym "nissl staining protocol"@en ;
    rdfs:subClassOf KSC_M:000009 .

KSC_M:0000011 rdf:type owl:Class ;
    rdfs:label "hematoxilin and eosin method"@en ;
    nsu:definition "Histochemical method for labeling the neurons. Hematoxilin labels the cell bodies, and eosin the cytoplasm. It is the most common method used in clinical and research labs."@en ;
    nsu:synonym "hematoxilin and eosin stain"@en ;
    rdfs:subClassOf nifstd:nlx_inv_20090605 .

KSC_M:0000012 rdf:type owl:Class ;
    rdfs:label "luxol fast blue method"@en ;
    nsu:definition "Histochemical method that stains in bluish color the myelin around axons."@en ;
    nsu:synonym "luxol fast blue staining"@en ;
    rdfs:subClassOf nifstd:nlx_inv_20090605 .

nifstd:birnlex_2243 rdf:type owl:Class ;
    rdfs:label "golgi method"@en ;
    nsu:definition "Classical cytochemical method used for neurons labeling. The method is based on the absorbtion of silver nitrates into the neurons, staining them entirely in deep brown to black. The stain is taken up by only maximum of 10% of the neurons in a labeled tissue."@en ;
    nsu:synonym "black reaction"@en ;
    nsu:synonym "golgi staining protocol"@en ;
    rdfs:subClassOf KSC_M:0000021 .

KSC_M:0000014 rdf:type owl:Class ;
    rdfs:label "golgi-cox method"@en ;
    nsu:definition "Variant of the Golgi method that uses mercury salts instead of silver. The labeling process takes 2 weeks, but the method is considered as the most reliable method from all Golgi techniques."@en ;
    rdfs:subClassOf KSC_M:0000021 .

KSC_M:0000015 rdf:type owl:Class ;
    rdfs:label "rapid golgi method"@en ;
    nsu:definition "Modification of the Golgi method that is faster than Golgi-Cox method. In this method osmium is added to the silver precipitation."@en ;
    rdfs:subClassOf KSC_M:0000021 .

KSC_M:0000016 rdf:type owl:Class ;
    rdfs:label "golgi-kopsch method"@en ;
    nsu:definition "Modification of the Golgi method where osmium that is used in rapid Golgi method is replaced by formaldehyde."@en ;
    rdfs:subClassOf KSC_M:0000021 .

KSC_M:0000017 rdf:type owl:Class ;
    rdfs:label "timm-danschle method"@en ;
    nsu:definition "Based on the property of the sulphide silver to reduce the metals present in trace amounts in cells to the corresponding metal sulphide and silver. The latter will form grains. The application of different types of chelating factors results in the bleaching of cells, or the opposite. It is suited for visualization at the light and electron microscope of synapses."@en ;
    rdfs:subClassOf obo:OBI_0302887 .

obo:OBI_0000854 rdf:type owl:Class ;
    rdfs:label "western blot"@en ;
    nsu:definition "A method from the category of immunoassays that is used to separate a protein from biological samples (cell or tissue), using gel electrophoresis. The protein is then transferred on a specific membrane (blot), conjugated with a primary and many times with a secondary antibody, which is usually labeled. Finally, an enzyme is applied, and the resultant label is then visualized and quantified."@en ;
    nsu:synonym "western blot analysis"@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:0000019 rdf:type owl:Class ;
    rdfs:label "kluver-barrera"@en ;
    nsu:definition "Extension of the Luxol Fast Blue method; the nuclei cell bodies are counterlabeled with cresyl violet or thionin."@en ;
    rdfs:subClassOf nifstd:nlx_inv_20090605 .

nifstd:nlx_inv_20090605 rdf:type owl:Class ;
    rdfs:label "histochemical method"@en ;
    nsu:definition "A class of staining methods applied to  tissues of interest. The used substances do not require antibodies, and the labeling shows the general organization of the tissue of interest, but not details of component cells"@en ;
    nsu:synonym "histochemical protocol"@en ;
    nsu:synonym "histochemistry"@en ;
    rdfs:subClassOf obo:OBI_0302887 .

KSC_M:0000021 rdf:type owl:Class ;
    rdfs:label "cytochemical method"@en ;
    nsu:definition "Class of staining methods that label the component cells of a tissue of interest in detail."@en ;
    rdfs:subClassOf obo:OBI_0302887 .

obo:ERO_0000214 rdf:type owl:Class ;
    rdfs:label "immunohistochemistry"@en ;
    nsu:definition "A class of methods used to label peptides and proteins in tissues for interest, using labeled antibodies. Immunohistochemistry is used to describe the spatial distribution of the protein of interest within tissue. It also can be used to measure the amount of protein in the tissues, but other techniques are more suited for this."@en ;
    nsu:synonym "immunohistochemistry assay"@en ;
    rdfs:subClassOf nifstd:nlx_inv_20090605 .

obo:ERO_0001240 rdf:type owl:Class ;
    rdfs:label "immunocytochemistry"@en ;
    nsu:definition "A class of methods used to label peptides and proteins in cells of interest, using labeled antibodies. Immunocytochemistry allows the description of the distribution of proteins and peptides in cells of interest."@en ;
    nsu:synonym "immunocytochemistry"@en ;
    rdfs:subClassOf KSC_M:0000021 .

KSC_M:0000024 rdf:type owl:Class ;
    rdfs:label "direct immunohistochemistry"@en ;
    nsu:definition "Class of methods used to label peptides and proteins in tissues for interest using a single (primary) antibody."@en ;
    rdfs:subClassOf obo:ERO_0000214 .

KSC_M:0000025 rdf:type owl:Class ;
    rdfs:label "indirect immunohistochemistry"@en ;
    nsu:definition "Class of methods used to label peptides and proteins in tissues of interest using a secondary antibody, which binds specifically to the primary, for signal amplification."@en ;
    rdfs:subClassOf obo:ERO_0000214 .

KSC_M:0000026 rdf:type owl:Class ;
    rdfs:label "enzymatic histochemistry"@en ;
    nsu:definition "Class of methods that use enzymes of specific peptides and proteins. The result of interaction is labeled. Examples: Acetylcholinesterase, cytochrome oxidase."@en ;
    rdfs:subClassOf obo:ERO_0000214 .

KSC_M:0000027 rdf:type owl:Class ;
    rdfs:label "immunohistochemistry for light microscopy"@en ;
    nsu:definition "Class of immunohistochemical methods which have their byproducts visible with the light microscope."@en ;
    rdfs:subClassOf obo:ERO_0000214 .

KSC_M:0000028 rdf:type owl:Class ;
    rdfs:label "avidin-biotinylated hrp complex method"@en ;
    nsu:definition "Immunohistochemical method that uses horseradish peroxidase (HRP) for visualization with the light microscope, and biotin conjugated with secondary antibodies."@en ;
    rdfs:subClassOf KSC_M:0000027 .

KSC_M:0000029 rdf:type owl:Class ;
    rdfs:label "peroxidase-antiperoxidase method"@en ;
    nsu:definition "Immunohistochemical method that uses the peroxidase-antiperoxidase complex to recognize the secondary antibody. This conjugated complex interacts enzymatically with specific molecules (e.g. HRP and DAB) and the colored product can be visualized with light microscope."@en ;
    rdfs:subClassOf obo:ERO_0000214 .

KSC_M:0000030 rdf:type owl:Class ;
    rdfs:label "immunohistochemistry for fluorescent microscopy"@en ;
    nsu:definition "Class of immunohistochemical methods for visualization with confocal microscope. These methods generally use two antibodies, and the fluorophore is conjugated with the second antibody."@en ;
    rdfs:subClassOf obo:ERO_0000214 .

KSC_M:0000031 rdf:type owl:Class ;
    rdfs:label "single labeling fluoro-immunohistochemistry"@en ;
    nsu:definition "Immunohistochemical method that uses two antibodies to describe the localization of a single protein of interest, which can be visualized with the confocal microscope."@en ;
    rdfs:subClassOf KSC_M:0000030 .

KSC_M:0000032 rdf:type owl:Class ;
    rdfs:label "multiple labeling fluoro-immunohistochemistry"@en ;
    nsu:definition "Immunohistochemical methods that are used to describe the co-localization of multiple proteins of interest, which can be visualized with the confocal microscope. The primary antibodies should not cross-react with each other. The fluorophores need to have minimal overlapping excitation/emission spectra one relative to the other, and from the background."@en ;
    rdfs:subClassOf KSC_M:0000030 .

KSC_M:0000033 rdf:type owl:Class ;
    rdfs:label "double labeling fluoro-immunohistochemistry"@en ;
    nsu:definition "Immunohistochemical method that is used to describe the co-localization of two proteins of interest, which can be visualized with the confocal microscope. The primary antibodies should not cross-react with each other. The fluorophores need to have minimal overlapping excitation/emission spectra, one relative to the other as well as from that of the background. This method allows identification of four phenotypes of cells in the tissue of interest."@en ;
    rdfs:subClassOf KSC_M:0000032 .

KSC_M:0000034 rdf:type owl:Class ;
    rdfs:label "triple labeling fluoro-immunohistochemistry"@en ;
    nsu:definition "Immunohistochemical method that is used to describe the co-localization of three proteins of interest, which can be visualized with the confocal microscope. The primary antibody should not cross-react. The fluorophores need to have minimal overlapping excitation/emission spectra one relative to the other, and from the background. This method allows identification of nine phenotypes of cells in the tissue of interest."@en ;
    rdfs:subClassOf KSC_M:0000032 .

KSC_M:0000035 rdf:type owl:Class ;
    rdfs:label "lucifer yellow labeling"@en ;
    nsu:definition "Cytochemical method that uses Lucifer Yellow for morphological characterization of cells. The labeling is performed by filling the cell with this fluorescent dye. It can be used in conjuction with electrophysiological experiments, and for study of gap juctions and glial cells. It is also used as in tract tracing experiments."@en ;
    nsu:synonym "lucifer yellow filling"@en ;
    rdfs:subClassOf nifstd:birnlex_2136 .

KSC_M:0000036 rdf:type owl:Class ;
    rdfs:label "immunohistochemistry for electron microscopy"@en ;
    nsu:definition "Immunohistochemical method similar to that for light microscopy, which is used to identify the antigen distribution into cell organelles. However, the slices for EM are very thin, they need to be better fixated, and the tissue has to be embedded in resin."@en ;
    rdfs:subClassOf obo:ERO_0000214 .

nifstd:birnlex_2136 rdf:type owl:Class ;
    rdfs:label "intracellular labeling method"@en ;
    nsu:definition "Class of cytochemical methods that use the intracellular filling as staining technique. The filling is done either in conjunction with electrophysiological measurements, or by applying electric fields that create pores in the cellular membrane (electroporation), etc. The stains that are used include horseradish peroxidase, Lucifer yellow, biocytin, etc."@en ;
    nsu:synonym "intracellular injection"@en ;
    rdfs:subClassOf KSC_M:0000021 .

KSC_M:0000038 rdf:type owl:Class ;
    rdfs:label "biocytin labeling"@en ;
    nsu:definition "Cytochemical method that uses biocytin for morphological characterization of cells. The labeling process is performed iontophoretically. The main advantages of this method are: the small size of the molecule, the molecule is (highly0 water soluble and it can be combinec with fluorescent markers. Biocytin can also be used as an anterograde tract-tracing stain."@en ;
    nsu:synonym "biocytin filling"@en ;
    nsu:synonym "biocytin tracing"@en ;
    rdfs:subClassOf nifstd:birnlex_2136 .

KSC_M:0000039 rdf:type owl:Class ;
    rdfs:label "autoradiographic labeling"@en ;
    nsu:definition "Class of tissue and nucleic acid labeling techniques that use radioactive isotopes as labels. The isotopes can be associated with ligands of the molecules of interest or included in them."@en ;
    rdfs:subClassOf obo:OBI_0302887 .

KSC_M:0000040 rdf:type owl:Class ;
    rdfs:label "radioligand binding assay"@en ;
    nsu:definition "Tissue labeling technique using radioactively-labeled ligands to receptors of interest. The ligands need to have high specifity, high value of the ratio between the specific binding and the nonspecific one, and a high specific activity."@en ;
    rdfs:subClassOf KSC_M:0000039 .

KSC_M:0000041 rdf:type owl:Class ;
    rdfs:label "competition assay"@en ;
    nsu:definition "Pharmacological method used to screen an array of drugs for their ability to bind to the receptor of interest."@en ;
    nsu:synonym "inhibition assay"@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:0000042 rdf:type owl:Class ;
    rdfs:label "inhibition assay"@en ;
    nsu:definition "Synonym of the competition assay, which is used to screen a set of drugs for their ability to bind to the receptor of interest."@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:ERO_0000716 rdf:type owl:Class ;
    rdfs:label "quantitative autoradiography"@en ;
    nsu:definition "The quantitative analysis of a specific radioligand binding at the microscopic, or electron microscopy level. It involves film exposure, which depends of the density of receptors, and of the radioactive atom."@en ;
    nsu:synonym "autoradiography"@en ;
    rdfs:subClassOf KSC_M:0000039 .

obo:ERO_0000771 rdf:type owl:Class ;
    rdfs:label "in situ hybridization"@en ;
    nsu:definition "Labeling and measurement of the mRNA or cDNA in neurons of a protein of interest using the complementary strand, which is labeled radioactively or non-radioactive labels such digoxygenin. It is used to define neuron classes based on the proteins or peptides it can expresses."@en ;
    nsu:synonym "in situ hybridization"@en ;
    rdfs:subClassOf KSC_M:00000225 ;
    rdfs:subClassOf obo:OBI_0302887 .

obo:ERO_0001168 rdf:type owl:Class ;
    rdfs:label "fluorescent in situ hybridization"@en ;
    nsu:definition "Method used to examine location of DNA within chromosomes, and identify chromosomal abnormalities, such as missing, mutated, or dislocated genetic segments."@en ;
    nsu:synonym "fluorescent in situ hybridization "@en ;
    rdfs:subClassOf obo:ERO_0000771 .

obo:OBI_0000415 rdf:type owl:Class ;
    rdfs:label "polymerase chain reaction"@en ;
    nsu:definition "Class of methods for DNA rapid amplification. Based on the DNA pairs enzymatic amplification mediated by primers and deoxynucleoside triphosphates (dNTPs), and catalyzed by DNA polymerase. PCR-based techniques are commonly used across many fields of biology."@en ;
    nsu:synonym "polymerase chain reaction"@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:ERO_0001075 rdf:type owl:Class ;
    rdfs:label "real time polymerase chain reaction"@en ;
    nsu:definition "PCR variant that measures the amplification at the end of each cycle i.e., in real time. Classical PCR measures the amplification at the end of the process. The RT-PCR uses a DNA-binding fluorescent dye, and its signal can be detected and measured at the end of each of amplification cycle. It is also called quantitative absolute PCR. "@en ;
    nsu:synonym "real-time pcr"@en ;
    rdfs:subClassOf obo:OBI_0000415 .

KSC_M:0000048 rdf:type owl:Class ;
    rdfs:label "traditional pcr"@en ;
    nsu:definition "The original PCR method, where amplified PCR products are detected at the end of all cycles. The method can not provide quantitative measures, but semi-quantitative, as a ratio between the yielded signal vs. the known signal of known concentrations. "@en ;
    rdfs:subClassOf obo:OBI_0000415 .

obo:OBI_0000552 rdf:type owl:Class ;
    rdfs:label "reverse transcription polymerase chain reaction"@en ;
    nsu:definition "Polymerase chain reaction method for the detection and semi-quantitative measure of the RNA."@en ;
    nsu:synonym "reverse transcribed polymerase chain reaction"@en ;
    rdfs:subClassOf obo:OBI_0000415 .

KSC_M:0000050 rdf:type owl:Class ;
    rdfs:label "real-time pcr with dna-binding dyes"@en ;
    nsu:definition "Real time PCR method using fluorescent dyes, which intercalate within the double-strand DNA and emits fluorescence. Multiplication of the double-strands increase the fluorescence. The dyes are not DNA-sequence specific."@en ;
    rdfs:subClassOf obo:ERO_0001075 .

KSC_M:0000051 rdf:type owl:Class ;
    rdfs:label "real-time pcr with fluorescent primers"@en ;
    nsu:definition "Real time PCR method using fluorophores directly attached to target specific primers Each time the primer is used to synthesize a new DNA strand, fluorescence intensity will increase."@en ;
    rdfs:subClassOf obo:ERO_0001075 .

KSC_M:0000052 rdf:type owl:Class ;
    rdfs:label "real-time pcr with fluorescent probes"@en ;
    nsu:definition "A very sensitive qt-PCR that uses two target specific primers (forward and reverse), and a probe. Because the primers are specific, this qt-PCR method is also very specific."@en ;
    rdfs:subClassOf obo:ERO_0001075 ;
    rdfs:subClassOf obo:ERO_0001075 .

KSC_M:0000053 rdf:type owl:Class ;
    rdfs:label "quantitative reverse-transcription polymerase chain reaction"@en ;
    nsu:definition "A very sensitive rt-PCR method used for detection and measurement of RNA and mRNA. "@en ;
    rdfs:subClassOf obo:OBI_0000552 .

KSC_M:0000054 rdf:type owl:Class ;
    rdfs:label "one step quantitative reverse-transcription polymerase chain reaction"@en ;
    nsu:definition "Both reverse transcription and qPCR are performed in one step in a single tube that contains the RNA, the reverse transcriptase and the DNA polymerase, and the other reverse transcription and the qPCR reagents."@en ;
    rdfs:subClassOf KSC_M:0000053 .

KSC_M:0000055 rdf:type owl:Class ;
    rdfs:label "two steps quantitative reverse-transcription polymerase chain reaction"@en ;
    nsu:definition "qrt-PCR method that first creates the DNA (cDNA) from the provided RNA using a transcriptase. In the second step, 10%-20% of the produced cDNA is used as template for the qtPCR."@en ;
    rdfs:subClassOf KSC_M:0000053 .

KSC_M:0000056 rdf:type owl:Class ;
    rdfs:label "multiplex polymerase chain reaction"@en ;
    nsu:definition "PCR method that contains multiple primers, therefore it is used to detect and measure several genes simultaneously. It is used for gene coexpression in a single cell."@en ;
    rdfs:subClassOf obo:OBI_0000415 .

KSC_M:0000057 rdf:type owl:Class ;
    rdfs:label "reverse transcriptase multiple polymerase chain reaction"@en ;
    nsu:definition "RT-PCR method for detection and measurement of several genes RNA."@en ;
    nsu:synonym "reverse transcribed polymerase chain reaction"@en ;
    rdfs:subClassOf obo:ERO_0001075 .

obo:ERO_0001298 rdf:type owl:Class ;
    rdfs:label "electrophysiological method"@en ;
    nsu:definition "Generic class of techniques employed to detect and measure the electrical activity of living cells (neurons), either individually or as ensembles. "@en ;
    nsu:synonym "electrophysiology assay"@en ;
    rdfs:subClassOf obo:ERO_0001319 .

nifstd:nlx_inv_090910 rdf:type owl:Class ;
    rdfs:label "intracellular recording"@en ;
    nsu:definition "Class of electrophysiological experiments (techniques) where the recording electrode is placed inside the cell (neuron)."@en ;
    nsu:synonym "intracellular electrode recording assay"@en ;
    rdfs:subClassOf obo:ERO_0001298 .

obo:ERO_0001685 rdf:type owl:Class ;
    rdfs:label "patch-clamp"@en ;
    nsu:definition "Class of electrophysiological experiments where the electrode is closely apposed to the neuronal membrane, forming a tight seal with a patch of the membrane."@en ;
    nsu:synonym "patch clamp technique"@en ;
    rdfs:subClassOf obo:ERO_0001298 .

KSC_M:0000061 rdf:type owl:Class ;
    rdfs:label "patch-clamp cell attached mode"@en ;
    nsu:definition "Patch-clamp method where a small amount of suction is applied to the back of the pipette. This creates a very tight seal between pipette and membrane in such way that the ions will not flow between the membrane and the pipette. The single electric current will be that of a single opened ion channel in the patched membrane, and that will flow into the pipette. The resulting electrical current can be measured with an amplifier."@en ;
    rdfs:subClassOf obo:ERO_0001685 .

nifstd:birnlex_2286 rdf:type owl:Class ;
    rdfs:label "patch-clamp inside-out"@en ;
    nsu:definition "Variant of the patch-clamp method where the pipette is retracted and the patch of membrane remains attached to the tip of it. It is used for studies of the influence of different intracellular molecules on the ion channels dynamics."@en ;
    nsu:synonym "cell-detached inside-out single-channel recording protocol"@en ;
    rdfs:subClassOf obo:ERO_0001685 .

KSC_M:0000063 rdf:type owl:Class ;
    rdfs:label "patch-clamp outside-out"@en ;
    nsu:definition "Variant of the patch-clamp method where the pipette is retracted and the attached membrane is exposed to the extracellular environment. It is used to studie the influence of different extratracellular molecules on the ion channels dynamics."@en ;
    rdfs:subClassOf obo:ERO_0001685 .

KSC_M:0000064 rdf:type owl:Class ;
    rdfs:label "patch-clamp perforated patch"@en ;
    nsu:definition "Variant of the patch-clamp method where the glass pipette is continuous with the cell, but instead of applying suction in the cell-attached mode to transition into the whole-cell mode, a chemical (often an antibiotic) is applied from within the glass pipette, causing small perforations in the membrane. It is an intermediate between the cell-attached mode and whole-cell mode."@en ;
    rdfs:subClassOf obo:ERO_0001685 .

KSC_M:0000065 rdf:type owl:Class ;
    rdfs:label "patch clamp whole cell"@en ;
    nsu:definition "Variant of the patch-clamp method where the glass pippete is continuous with the cytoplasm of the recorded cell. Used to measure the potentials and currents of the whole cell, and to inject different substances directly into the interior of the cell."@en ;
    rdfs:subClassOf obo:ERO_0001685 .

nifstd:birnlex_2272 rdf:type owl:Class ;
    rdfs:label "extracellular recording"@en ;
    nsu:definition "Class of electrophysiological methods that detect and measure field potentials such as evoked and behavior-related field potential recordings, and compound spikes (compound action potentials), as well as individual units recordings. The recording electrode is placed outside of the target neuron."@en ;
    nsu:synonym "extracellular electrode recording protocol"@en ;
    rdfs:subClassOf obo:ERO_0001298 .

nifstd:birnlex_2276 rdf:type owl:Class ;
    rdfs:label "extracellular single unit recording"@en ;
    nsu:definition "Extracellular electrophysiological methods where the recording electrode is placed near the cell soma or the initial segment. It is used to record (evoked or spontaneous) action potentials and measure variables of the action potential such as amplitude and latency."@en ;
    nsu:synonym "single electrode extracellular recording protocol"@en ;
    rdfs:subClassOf nifstd:birnlex_2272 .

KSC_M:0000068 rdf:type owl:Class ;
    rdfs:label "reporter genes method"@en ;
    nsu:definition "Class of methods for visualization and measurement of gene expression patterns using non-endogenous genes that are encoding for enzymes or fluorescent proteins whose expressions are controlled by the promoters for the gene of interest (different than the reporter gene). These genes are either directly controlled by the promoters of the genes of interest, or the DNA sequence of the reporter is fused with the DNA for the gene of interest (piggy-back), and both will be under the control of the promoter."@en ;
    rdfs:subClassOf KSC_M:00000225 ;
    rdfs:subClassOf KSC_M:000004 .

KSC_M:0000069 rdf:type owl:Class ;
    rdfs:label "enyzme-linked immunosorbent assay"@en ;
    nsu:definition "Biochemical assay where a specific antibody binds to a protein of interest to mark the presence and measure the quantity of the protein in a sample."@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:ERO_0000652 rdf:type owl:Class ;
    rdfs:label "radioimmunoassay"@en ;
    nsu:definition "Biochemical assay where an antibody binds to radioactive proteins of known concentration. A sample containing nonradioactive proteins is added to compete with the radioactive proteins to the antibody loci, allowing determination of the protein concentration in the added sample."@en ;
    nsu:synonym "radioimmunoassay"@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:OBI_0000690 rdf:type owl:Class ;
    rdfs:label "immunoprecipitation"@en ;
    nsu:definition "Biochemical method used for proteins purification. Specific antibodies are bound to a mixture of beads and added to the solution. The antibody-conjugated beads will bind to the proteins of interest and the beads will precipitate the antibody-protein of interest from the solution. The final step will be the removal of antibodies, usually by changing the salts concentration or the pH."@en ;
    nsu:synonym "immunoprecipitation"@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:0000072 rdf:type owl:Class ;
    rdfs:label "chromatography"@en ;
    nsu:definition "Class of purification methods applied to a wide range of biomolecules."@en ;
    rdfs:subClassOf :nsm_0000000 .

KSC_M:0000073 rdf:type owl:Class ;
    rdfs:label "gel filtration chromatography"@en ;
    nsu:definition "Purification method where the molecules of interest are run through through a column and are separated by their size."@en ;
    rdfs:subClassOf KSC_M:0000072 .

obo:ERO_0001501 rdf:type owl:Class ;
    rdfs:label "ion exchange chromatography"@en ;
    nsu:definition "Purification method where the molecules are separated by their sizes of their electrical charges."@en ;
    nsu:synonym "ion chromatography "@en ;
    rdfs:subClassOf KSC_M:0000072 .

KSC_M:0000075 rdf:type owl:Class ;
    rdfs:label "affinity chromatography"@en ;
    nsu:definition "Purification method where the molecules are run through a matrix. Small molecules are fixed to the porous matrix to bind to molecules that may have a high affinity, such as a substrate binding to an enzyme."@en ;
    rdfs:subClassOf KSC_M:0000072 .

obo:ERO_0001034 rdf:type owl:Class ;
    rdfs:label "coimmunoprecipitation"@en ;
    nsu:definition "Variant (refinement) of the immunoprecipitation where a second antibody is used in addition to the primary antibody-beads complex. The second antibody is added after the protein is eluted from the solution, and in conjunction with Western blot or ELISA methods is used to detect the presence of any secondary proteins that may interact with the immunoprecipitated protein."@en ;
    nsu:synonym "coimmunoprecipitation"@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:ERO_0000926 rdf:type owl:Class ;
    rdfs:label "chromatin immunoprecipitation"@en ;
    nsu:definition "Combination of immunoprecipitation and chromatography where the antibody is bound to small beads in a column. The protein is bound by the antibody and purified through the column. Polymerase chain reaction (PCR) can be used subsequently, to determine the presence of any bound DNA sequences. Used to determine whether a protein can directly interact with a specific region of DNA."@en ;
    nsu:synonym "chromatin immunoprecipitation"@en ;
    rdfs:subClassOf obo:OBI_0000690 .

KSC_M:0000078 rdf:type owl:Class ;
    rdfs:label "antibody interference"@en ;
    nsu:definition "Purification method where the antibody is bound to the protein, which will interfere with its function."@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:OBI_0000470 rdf:type owl:Class ;
    rdfs:label "mass spectometry"@en ;
    nsu:definition "Technique used to identify and sequence peptides and proteins done by computing the mass to charge ratio. It is used in combination with databases of proteins and their properties, to detect unknown aminoacids sequences. The solution needs to be in gaseous phase, and the aminoacids sequences charged."@en ;
    nsu:synonym "mass spectrometry assay"@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:0000080 rdf:type owl:Class ;
    rdfs:label "matrix-assisted laser desorption ionization"@en ;
    nsu:definition "Technique used to bring the aminoacids sequence solution in gaseous state and charged. The sequence of interest is mixed with a UV-absorbing organic acid and dried on a metal slide. The sample is irradiated with a UV laser, causing the solution to go into the gas phase. The aminoacids sequence also becomes charged. This method precludes the mass spectometry technique."@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:ERO_0001331 rdf:type owl:Class ;
    rdfs:label "electrospray ionization"@en ;
    nsu:definition "Technique used to bring aminoacids sequences solutions in gaseous state and charged. The acidic solution is passed through capillaries into a heated area where the solution is transformed into gas, and molecules of interest become charged. This technique preclude the mass spectometry assay."@en ;
    nsu:synonym "electrospray ionization"@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:0000082 rdf:type owl:Class ;
    rdfs:label "collision induced dissociation"@en ;
    nsu:definition "Technique used to fragment proteins that have the mass to charge ratio identical, by bombarding with atoms (e.g. argon, nitrogen). The resulting fragments are the run through mass spectometers. The proteins are then reconstituted by overlapping the fragments."@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:0000083 rdf:type owl:Class ;
    rdfs:label "kinase assay"@en ;
    nsu:definition "Technique used to determine whether a protein phosphorylates another protein (the substrate), by adding radioactive ATP. The radioactive phosphorus is transferred to the protein substrate, which is then immunoprecipitated. The purified phosphorylated protein is visualized using autoradiography. "@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:ERO_0000907 rdf:type owl:Class ;
    rdfs:label "electrophoretic mobility shift assay"@en ;
    nsu:definition "Technique used to determine whether a protein can directly interact with a short and determined DNA sequence. Also known as gel shift assay."@en ;
    nsu:synonym "electrophoretic gel mobility shift"@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:0000085 rdf:type owl:Class ;
    rdfs:label "luciferase assay"@en ;
    nsu:definition "Technique used to determine whether a protein can activate or repress the expression of a target gene and the amount of the gene product, by fusing the luciferase DNA to the regulatory region of the target protein DNA. However, it cannot determine if the protein directly interacts with the DNA or indirectly affects transcription by activating or repressing an intermediary protein that controls the transcription."@en ;
    rdfs:subClassOf KSC_M:00000225 .

obo:ERO_0000215 rdf:type owl:Class ;
    rdfs:label "light microscopy"@en ;
    nsu:definition "Class of techniques involving a microscope that uses visible light to illuminate and image a specimen."@en ;
    nsu:synonym "light microscopy"@en ;
    rdfs:subClassOf obo:ERO_0000201 .

KSC_M:0000087 rdf:type owl:Class ;
    rdfs:label "brightfield microscopy"@en ;
    nsu:definition "The most common and general form of light microscopy, using a simple or compound microscope. The visible light passes directly through or is reflected off a specimen. "@en ;
    rdfs:subClassOf obo:ERO_0000215 .

obo:ERO_0001636 rdf:type owl:Class ;
    rdfs:label "phase-contrast microscopy"@en ;
    nsu:definition "Phase-contrast microscopy uses indices of refraction of different media through which the light passes, or is refracted and reflected. This technique amplifies the differences in phase generated by the indices of refraction into larger intensity differences with high contrast. It is commonly used for visualization of cell cultures."@en ;
    nsu:synonym "phase contrast microscopy"@en ;
    rdfs:subClassOf obo:ERO_0000215 .

KSC_M:0000089 rdf:type owl:Class ;
    rdfs:label "darkfield microscopy"@en ;
    nsu:definition "This method uses oblique illuminating rays of light directed from the side so that only scattered light enters the objective lens. The objects that are transparent will appear dark, while those that reflect light will be very bright. Used in neuroanatomy for visualization of fiber tracts in Nissl stained slides. Also called darkground microscopy. Also known as Nomarski method."@en ;
    rdfs:subClassOf obo:ERO_0000215 .

obo:ERO_0001637 rdf:type owl:Class ;
    rdfs:label "differential interference contrast microscopy"@en ;
    nsu:definition "Variant of the phase-contrast microscopy that uses the indices of refraction of different media to create three-dimensional textured look of the cellular components."@en ;
    nsu:synonym "differential interference contrast microscopy"@en ;
    rdfs:subClassOf obo:ERO_0000215 .

obo:ERO_0001356 rdf:type owl:Class ;
    rdfs:label "fluorescence microscopy"@en ;
    nsu:definition "Class of microscopy methods that visualize and magnify cells and their parts, which are labeled or express a class of substances called fluorophores. Fluorophores absorb (are excited) by specific light wavelengths and emit light with different wavelengths, usually higher. "@en ;
    nsu:synonym "fluorescent microscopy"@en ;
    rdfs:subClassOf obo:ERO_0000201 .

nifstd:birnlex_3077 rdf:type owl:Class ;
    rdfs:label "wide field fluorescence microscopy"@en ;
    nsu:definition "The most common fluorescence microscopy. Also called epifluorescence."@en ;
    nsu:synonym "wide-field fluorescence imaging protocol"@en ;
    rdfs:subClassOf obo:ERO_0001356 .

obo:ERO_0000200 rdf:type owl:Class ;
    rdfs:label "confocal microscopy"@en ;
    nsu:definition "Fluorescence microscopy method that is used to produce images of structures within specimens by selectively collecting light from thin (<1 ?m) regions. This is realized by aligning the pinhole detector aperture with the pinhole illuminating aperture in such way that they are confocal."@en ;
    nsu:synonym "confocal microscopy"@en ;
    rdfs:subClassOf obo:ERO_0001356 .

obo:ERO_0001640 rdf:type owl:Class ;
    rdfs:label "two-photon microscopy"@en ;
    nsu:definition "Fluorescence microscopy method that uses 2 photons to excite the fluorophores in the specimen, each having half of the frequency (double the wavelength). Also known as two-photon laser scanning microscopy (TPLSM)."@en ;
    nsu:synonym "multiphoton microscopy"@en ;
    rdfs:subClassOf obo:ERO_0001356 .

nifstd:birnlex_2264 rdf:type owl:Class ;
    rdfs:label "total internal reflection fluorescent microscopy"@en ;
    nsu:definition "Fluorescence microscopy method where the section of detection is fixed at the interface between the specimen and the surface the specimen is mounted on. It is used for visualization and magnification of cell surfaces."@en ;
    nsu:synonym "total internal reflection imaging protocol"@en ;
    rdfs:subClassOf obo:ERO_0001356 .

obo:ERO_0000328 rdf:type owl:Class ;
    rdfs:label "electron microscopy"@en ;
    nsu:definition "Microscopy method that uses focused beams of electrons instead of photons, thus reducing the minimum wavelength and increasing the resolving power to the scale of tenths of nanometers."@en ;
    nsu:synonym "electron microscopy"@en ;
    rdfs:subClassOf obo:ERO_0000201 .

obo:ERO_0000329 rdf:type owl:Class ;
    rdfs:label "transmission electron microscopy"@en ;
    nsu:definition "Electron microscopy method that uses heavy metals staining of very thin sections of the specimen. The atoms of the heavy metals will absorb or scatter the electrons beam. The variations of the electron beam intensity are transformed into photons, which will produce two-dimensional images of the thin sections."@en ;
    nsu:synonym "transmission electron microscopy"@en ;
    rdfs:subClassOf obo:ERO_0000328 .

obo:ERO_0000330 rdf:type owl:Class ;
    rdfs:label "scanning electron microscopy"@en ;
    nsu:definition "Electron microscopy method that uses high-energy electron beams to scan the surface of the specimen, which is coated by a gold or silver layer. The interaction between the electrons and the specimen surface is converted into electric signals that are converted in pixels on a screen. The resulting image is a 3D rendering of the specimen's surface topography."@en ;
    nsu:synonym "scanning electron microscopy"@en ;
    rdfs:subClassOf obo:ERO_0000328 .

KSC_M:0000099 rdf:type owl:Class ;
    rdfs:label "infrared differential interference contrast microscopy"@en ;
    nsu:definition "Variant of the differentation interference contrast microscopy where the instead of light, infrared waves are used. This method is a combination of microscopy and spectrometry for infrared, which includes Fourier Transformation for transformation of IR into visible light."@en ;
    rdfs:subClassOf KSC_M:000004 .

nifstd:birnlex_2865 rdf:type owl:Class ;
    rdfs:label "multichannel recording"@en ;
    nsu:definition "Multiple electrodes electrophysiological methods that are used to record neuron populations. The basic configurations are the stereotrode (2 recording wires) and tetrodes (4 recording electrodes). Combinations of these configurations allow simultaneous recordings from 32 or 64 sites. "@en ;
    nsu:synonym "multiple electrode extracellular recording protocol"@en ;
    rdfs:subClassOf nifstd:birnlex_2272 .

nifstd:nlx_152558 rdf:type owl:Class ;
    rdfs:label "optogenetics"@en ;
    nsu:definition "The combination of genetic and optical methods to achieve gain and loss of function of well-defined events in the specific cells of the living tisssue (Deisseroth, 2011)"@en ;
    rdfs:subClassOf KSC_M:00000225 ;
    rdfs:subClassOf KSC_M:000004 .

KSC_M:00000102 rdf:type owl:Class ;
    rdfs:label "voltage-sensitive dyes imaging"@en ;
    nsu:definition "Class of methods that use different molecules to visualize and measure variations of the electric potential in tissue, either in single cells or of cellular ensembles."@en ;
    rdfs:subClassOf KSC_M:000004 ;
    rdfs:subClassOf obo:OBI_0302887 .

KSC_M:00000103 rdf:type owl:Class ;
    rdfs:label "fluorescent dyes imaging"@en ;
    nsu:definition "Class of voltage-sensitive dyes imaging methods where different synthetic fluorescent probes are used. They can be further subdivided in fast- and slow-sensitive methods, depending on whether they can detect fast vs slow electrical changes."@en ;
    rdfs:subClassOf KSC_M:00000102 .

obo:ERO_0000634 rdf:type owl:Class ;
    rdfs:label "calcium-sensitive dyes imaging"@en ;
    nsu:definition "Class of voltage sensitive dyes imagining methods that use the intracellular calcium as reporter for electrical activity of single or ensembles of cells. The intracelular Ca concentration increases during an action potential, therefore the fluroescent signal given by dyes that bound with it will increase."@en ;
    nsu:synonym "calcium imaging assay"@en ;
    rdfs:subClassOf KSC_M:00000103 .

KSC_M:00000105 rdf:type owl:Class ;
    rdfs:label "permeable calcium-sensitive dyes imaging"@en ;
    nsu:definition "Class of calcium voltage-sensitive dyes imagining methods that use fluorescent probes that can cross the membrane."@en ;
    rdfs:subClassOf obo:ERO_0000634 .

KSC_M:00000106 rdf:type owl:Class ;
    rdfs:label "multi-cell bolus loading"@en ;
    nsu:definition "Method of labeling active neurons with membrane-permeant calcium-indicator fluorescent dyes. Used for monitorization of neuronal ensembles."@en ;
    rdfs:subClassOf KSC_M:00000107 .

KSC_M:00000107 rdf:type owl:Class ;
    rdfs:label "impermeable calcium-sensitive dyes imaging"@en ;
    nsu:definition "Class of calcium-indicator fluorescent dyes imaging methods that use impermeable dyes, which are injected into neurons either through a recording electrode, or using electroporation."@en ;
    rdfs:subClassOf obo:ERO_0000634 .

obo:OBI_0302912 rdf:type owl:Class ;
    rdfs:label "electroporation"@en ;
    nsu:definition "Class of methods that use strong electric fields to change the membrane permeability of a cell. Depending on the intensity, mode, time of application of the external electric field, temperature and presence of specific molecules, the induced pores into the cell membrane can be transient or metastable."@en ;
    nsu:synonym "electroporation"@en ;
    rdfs:subClassOf :nsm_0000000 .

KSC_M:00000109 rdf:type owl:Class ;
    rdfs:label "genetic encoders of calcium imaging"@en ;
    nsu:definition "Generic class of methods that use chimeric fluorescent proteins (opsins) that can be used to monitor variations of intracellular calcium concentrations. GECIs consist of a calcium-binding domain (calmodulin or troponin C), fused to one or two fluorescent proteins."@en ;
    rdfs:subClassOf nifstd:nlx_152558 .

KSC_M:00000110 rdf:type owl:Class ;
    rdfs:label "single fluorescent protein genetic encoder of calcium imaging"@en ;
    nsu:definition "Optogenetic method that uses a single fluorescent protein to visualize intracellular calcium. Usually, the fluorescence sensitivity is increased when the fluorescent protein is split in two (circularly permutation) and its conformation is modulated by calcium bindingdependent changes in the chromophore environment."@en ;
    rdfs:subClassOf KSC_M:00000109 .

KSC_M:00000111 rdf:type owl:Class ;
    rdfs:label "two fluorescent proteins genetic encoder of calcium imaging"@en ;
    nsu:definition "Optogenetic method that uses two variants of a fluorescent protein modified in such way that their spectral properties allow a specific resonance transfer energy (Forster FRET)."@en ;
    rdfs:subClassOf KSC_M:00000109 .

KSC_M:00000112 rdf:type owl:Class ;
    rdfs:label "acute gene transfer"@en ;
    nsu:definition "Class of methods that use viruses for transfer of specific genes into the DNA of the cell of of interest. The most used viruses are retroviruses (lentiviruses) and adeno-associated viruses (AAV)."@en ;
    rdfs:subClassOf :nsm_0000000 .

KSC_M:00000113 rdf:type owl:Class ;
    rdfs:label "sampling method"@en ;
    nsu:definition "Class of methods used to select few units that will provide the required estimates of a variable of interest for an entire population. Generally, sampling methods are statistic or non-statistic."@en ;
    nsu:synonym "sampling"@en ;
    rdfs:subClassOf KSC_M:000006 .

nifstd:nlx_inv_090919 rdf:type owl:Class ;
    rdfs:label "tract tracing"@en ;
    nsu:definition "Class of methods used to unravel neuroanatomical connections at the neuronal population level."@en ;
    nsu:synonym "tract tracing assay"@en ;
    rdfs:subClassOf obo:OBI_0302887 .

KSC_M:00000115 rdf:type owl:Class ;
    rdfs:label "degeneration method"@en ;
    nsu:definition "Class of tract tracing methods that use lesions of myelinated fiber tracts to identify neuroanatomical connections. The lesions of the myelinated axons lead to their degeneration, which is visualized against the background of gray matter."@en ;
    rdfs:subClassOf nifstd:nlx_inv_090919 .

KSC_M:00000116 rdf:type owl:Class ;
    rdfs:label "wallerian degeneration"@en ;
    nsu:definition "Tract tracing method where the course and terminations of fibers are identified by the anterograde degeneration of the myelin and of distal parts of the lesioned axons."@en ;
    rdfs:subClassOf KSC_M:00000115 ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000117 rdf:type owl:Class ;
    rdfs:label "marchi method"@en ;
    nsu:definition "Anterograde degeneration tract tracing method where course and termination of neuroanatomical fibers are identified using osmic acid staining of the degenerated myelin."@en ;
    rdfs:subClassOf KSC_M:00000115 ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000118 rdf:type owl:Class ;
    rdfs:label "cell reaction method"@en ;
    nsu:definition "Retrograde degeneration tract tracing method where the origin of neuroanatomical connections (axons proximal parts and cell bodies) is identified after lesioning the fiber tracts."@en ;
    rdfs:subClassOf obo:ERO_0001277 ;
    rdfs:subClassOf KSC_M:00000115 .

KSC_M:00000119 rdf:type owl:Class ;
    rdfs:label "nauta method"@en ;
    nsu:definition "Tract tracing method where the neuroanatomical connections are identified by silver impregnation of the degenerating fibers."@en ;
    rdfs:subClassOf KSC_M:00000115 ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000120 rdf:type owl:Class ;
    rdfs:label "nauta-gygax method"@en ;
    nsu:definition "Variant of the original Nauta method. The impregnated degenerating axons appear black against the background of normal fibers. Also known as the suppresive Nauta-Gygax method, or NautaGygax enhanced silver tracing method."@en ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000121 rdf:type owl:Class ;
    rdfs:label "glees method"@en ;
    nsu:definition "Variant of the Nauta method used for identification of the silver stained degenerating axons terminations structures. Can be used in conjuction with electron microscopy."@en ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000122 rdf:type owl:Class ;
    rdfs:label "fink-heimer method"@en ;
    nsu:definition "Variant of the Nauta method that additionally uses potassium permanganate-uranyl nitrate sequence, which allows bettter visualization of degenerating axons, their terminations, and the terminal boutons (boutons terminaux)."@en ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000123 rdf:type owl:Class ;
    rdfs:label "anterograde tracing"@en ;
    nsu:definition "Class of tract tracing methods that label the axons and their terminations either by anterograde axonal transport or degeneration."@en ;
    rdfs:subClassOf nifstd:nlx_inv_090919 .

obo:ERO_0001277 rdf:type owl:Class ;
    rdfs:label "retrograde tracing"@en ;
    nsu:definition "Class of tract tracing methods that label the origins (cell bodies) of the neuroanatomical connections either by retrograde axonal transport, or by lesions."@en ;
    nsu:synonym "retrograde labeling"@en ;
    rdfs:subClassOf nifstd:nlx_inv_090919 .

KSC_M:00000125 rdf:type owl:Class ;
    rdfs:label "anterograde and retrograde tract tracing"@en ;
    nsu:definition "Class of tract tracing methods that label both the axonal terminations and the cell bodies using both modes of axonal transport or lesions."@en ;
    rdfs:subClassOf nifstd:nlx_inv_090919 .

KSC_M:00000126 rdf:type owl:Class ;
    rdfs:label "tritiated aminoacids tracing"@en ;
    nsu:definition "Anterograde tract tracing method using radioactive aminoacids, which are delivered by pressure injection into the region of interest. The aminoacids are transported anterogradely towards the axonal terminals, which can be visualized onto film. The course of labeled axons cannot be determined directly. The radioactive isotopes that are used usually have short decaying time (half time). Also known as orthograde axoplasmic transport autoradiographic tracing (OOAT)."@en ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000127 rdf:type owl:Class ;
    rdfs:label "horseradish peroxidase tracing"@en ;
    nsu:definition "Both anterograde and retrograde tract tracing method that uses horseradish peroxidase as stain for axon terminals and neuronal cell bodies, respectively. However, HRP tracing is used mostly retrogradely. The HRP presence is detected using classical immunohistochemistry"@en ;
    rdfs:subClassOf KSC_M:00000125 .

KSC_M:00000128 rdf:type owl:Class ;
    rdfs:label "radioactive and non-radioactive hrp tracing"@en ;
    nsu:definition "Combination of the anterograde tritiated aminoacids and of HRP tracing methods, used for visualization of axon collaterals. The radioactive HRP is enzymatically inactive, and the non-radioactive HRP is enzymatically active."@en ;
    rdfs:subClassOf KSC_M:00000125 .

KSC_M:00000129 rdf:type owl:Class ;
    rdfs:label "retrograde fluorescent tracing"@en ;
    nsu:definition "Class of tract tracing methods that use retrogradely transported fluorescent markers. Some of the most commonly used markers are Evans blue, Fast blue, Diamidino Yellow (DI), nuclear yellow, fluorogold (FG). These markers can be used as single tracers, in combinations (e.g., DI and FG),  or with toxins-based tracers, and with anterograde tracers."@en ;
    rdfs:subClassOf obo:ERO_0001277 .

KSC_M:00000130 rdf:type owl:Class ;
    rdfs:label "wheat-germ agglutinin and horseradish peroxidase tracing"@en ;
    nsu:definition "Combination of WGA and HRP as tract tracer, which can be used both anterogradely and retrogradely."@en ;
    rdfs:subClassOf KSC_M:00000125 .

KSC_M:00000131 rdf:type owl:Class ;
    rdfs:label "cholera toxin tracing"@en ;
    nsu:definition "Class of tracing methods that use bacterial toxins conjugated with HRP, fluorescein, or rhodamine beads. The most used bacterial toxis is  non-toxic B fragment of cholera toxin (CTB). The detection is immunohistochemical, therefore this method is more sensitive than HRP. It can be used both anterogradely and retrogradely."@en ;
    rdfs:subClassOf obo:ERO_0001277 .

KSC_M:00000132 rdf:type owl:Class ;
    rdfs:label "micro- and nano particles retrograde tracing"@en ;
    nsu:definition "Class of tracing methods that use small particles composed of a label-coated material, which can employed alone, or conjugated with other tracers."@en ;
    rdfs:subClassOf obo:ERO_0001277 .

KSC_M:00000133 rdf:type owl:Class ;
    rdfs:label "rhodamine beads tracing"@en ;
    nsu:definition "Retrograde tracing method that uses fluorescent rhodamine carrying latex beads."@en ;
    rdfs:subClassOf obo:ERO_0001277 .

KSC_M:00000134 rdf:type owl:Class ;
    rdfs:label "gold particles coated with wheat-germ conjugated apohrp"@en ;
    nsu:definition "Retrograde tracing method that combines WGA-HRP with gold particles (10-20 nm). It can be viewed directly through a light microscope and used in EM."@en ;
    rdfs:subClassOf obo:ERO_0001277 .

KSC_M:00000135 rdf:type owl:Class ;
    rdfs:label "diolistic labeling"@en ;
    nsu:definition "Neurons labeling method with lipophilic carbocyanine dyes that are injected into the cells of interest. These dyes label efficiently the neurons membranes, providing a more accurate view of dendrites and dendritic spines. It can be used in combination with retrograde tracers."@en ;
    rdfs:subClassOf nifstd:birnlex_2136 .

KSC_M:00000136 rdf:type owl:Class ;
    rdfs:label "retrograde tracing of diolistic neurons"@en ;
    nsu:definition "Combination of retrograde tracing methods with microparticles or fluorescent tracers (e.g. fluorogold) with the intracellular filling of neurons with lipophilic carbocyanine dyes."@en ;
    rdfs:subClassOf obo:ERO_0001277 .

KSC_M:00000137 rdf:type owl:Class ;
    rdfs:label "anterograde fluorescent tracing"@en ;
    nsu:definition "Class of tract tracing methods that use anterogradely transported fluorescent markers. Some of the most commonly used markers are biotinylated dextran amine (BDA), Phaseolus vulgaris leucoagglutinin (PHAL), Lucifer yellow-dextran, fluorescein-dextran, and Fluoro Ruby (tetramethylrhodamine-dextran), neurobiotin, and biocytin. One of the advantages of these markers vs the tritiated aminoacids is that the course and terminations of labeled axons can be directly visualized."@en ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000138 rdf:type owl:Class ;
    rdfs:label "biotinylated dextran amine tracing"@en ;
    nsu:definition "Anterograde tracing method using BDA as marker. The 10kDA BDA is one of the best markers for anterograde tracing in different species, while the 3kDA BDA is used for cell filling, and for retrograde tracing."@en ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000139 rdf:type owl:Class ;
    rdfs:label "phaseolus vulgaris leucoagglutinin tracing"@en ;
    nsu:definition "Anterograde tracing method using PHAL as marker, which is considered the \"gold standard\" at least in small animals. PHA-L labels the axons only anterogradely when iontophoretic injections are employed, and there very few cases of dendrites labeling when the marker was pressure-injected."@en ;
    rdfs:subClassOf KSC_M:00000123 .

KSC_M:00000140 rdf:type owl:Class ;
    rdfs:label "coinjection tract tracing"@en ;
    nsu:definition "Tracing method that involves the injection of two markers in the same injection site. Usually, one anterograde and one retrograde tracer are injected, for simultaneous detection of both inputs and outputs of region's of interest."@en ;
    rdfs:subClassOf KSC_M:00000125 .

KSC_M:00000141 rdf:type owl:Class ;
    rdfs:label "double coinjection tract tracing"@en ;
    nsu:definition "Tracing method that involves injections of two pairs of stains, in two different regions. Each coinjection includes one anterograde and one retrograde tracer. This method is used for unraveling and visualization of regions' of interest networks."@en ;
    rdfs:subClassOf KSC_M:00000125 .

KSC_M:00000142 rdf:type owl:Class ;
    rdfs:label "transsynaptic tracing"@en ;
    nsu:definition "Class of tracing methods that label connections across more than 1 node."@en ;
    rdfs:subClassOf nifstd:nlx_inv_090919 .

KSC_M:00000143 rdf:type owl:Class ;
    rdfs:label "degeneration transsynaptic tracing"@en ;
    nsu:definition "Tracing methods based on lesions of tracts that induce degeneration of fibers beyond the first synaptic station (terminal field). "@en ;
    rdfs:subClassOf KSC_M:00000115 ;
    rdfs:subClassOf KSC_M:00000123 ;
    rdfs:subClassOf KSC_M:00000142 .

KSC_M:00000144 rdf:type owl:Class ;
    rdfs:label "viruses transsynaptic tracing"@en ;
    nsu:definition "Tracing methods that use viruses (e.g., PrV and Bartha) as markers, and which label connections beyond the first synaptic station. Viruses that allow transneuronal labeling include Herpes simplex (HSV1) and pseudorabies virus (PRV), and the CSV strain of the rabies virus."@en ;
    rdfs:subClassOf KSC_M:00000125 ;
    rdfs:subClassOf KSC_M:00000142 .

KSC_M:00000145 rdf:type owl:Class ;
    rdfs:label "double-virus translabeling neuron method"@en ;
    nsu:definition "Virus transsynaptic tracing method that uses two different viruses to characterize functionally related networks. Each virus is genetically modified in such way that it expresses a specific antigen. The network infected by each virus is identified immunohistochemically, and the common nodes will be doubled labeled."@en ;
    rdfs:subClassOf KSC_M:00000144 .

KSC_M:00000146 rdf:type owl:Class ;
    rdfs:label "double recombinant viral transsynaptic tracing"@en ;
    nsu:definition "Virus transsynaptic tracing method that uses two strains of recombinant rabies virus (RV), each of them modified in such that it will express an unique marker in infected neurons. The RV propagates retrogradely, and if injected in two regions, the network infected by each strain will include the associated marker. The identification of each network is performed histochemically, and the common nodes will be double labeled."@en ;
    rdfs:subClassOf KSC_M:00000144 .

KSC_M:00000147 rdf:type owl:Class ;
    rdfs:label "golgi-like retrograde labeling using viruses"@en ;
    nsu:definition "Cytochemical labeling methods that uses retrograde tracing with retrogradely-propagating viruses followed by the antibody-based detection of the infected neuron."@en ;
    rdfs:subClassOf nifstd:birnlex_2136 .

KSC_M:00000148 rdf:type owl:Class ;
    rdfs:label "golgi-like retrograde labeling using rabies virus"@en ;
    nsu:definition "Golgi-like retrograde labeling method that uses the rabies virus to label a neuron, followed by a specific antibody-based detection of the infected neuron. This method has the advantage of not labeling fibers of passage."@en ;
    rdfs:subClassOf KSC_M:00000147 .

KSC_M:00000149 rdf:type owl:Class ;
    rdfs:label "golgi-like retrograde labeling using herpes virus"@en ;
    nsu:definition "Golgi-like retrograde labeling method that uses the herpes virus to label a neuron, followed by a specific antibody-based detection of the infected neuron."@en ;
    rdfs:subClassOf KSC_M:00000147 .

KSC_M:00000150 rdf:type owl:Class ;
    rdfs:label "single cell juxtacellular recording-labeling technique"@en ;
    nsu:definition "Combination of the intracellular filling method and extracellular electrophysiological method. The pipette is used both for iontophoretic filling of the cell of interest, and as an electrode for monitoring the electrophysiological activity of the neuron. The pipette should be in juxtaposition with the cells' membrane. It can be used for morphological recostruction and electrophysiological monitoring of micromodules made of adjacent neurons."@en ;
    rdfs:subClassOf nifstd:birnlex_2136 .

KSC_M:00000151 rdf:type owl:Class ;
    rdfs:label "loose-patch-juxtacellular recording"@en ;
    nsu:definition "Variant of the single cell juxtacellular recording-labeling technique"@en ;
    rdfs:subClassOf nifstd:birnlex_2136 .

KSC_M:00000152 rdf:type owl:Class ;
    rdfs:label "bioimaging"@en ;
    nsu:definition "General class of non-invasely techniques used to visualize structures inside the body."@en ;
    rdfs:subClassOf KSC_M:000004 .

obo:ERO_0001235 rdf:type owl:Class ;
    rdfs:label "magnetic resonance imaging"@en ;
    nsu:definition "Imaging techniques based on application of strong magnetic fields that are used to visualize the structure and physiology of different organs or parts of them, both in healthy (normal) subjects and in diseased subjects."@en ;
    nsu:synonym "magnetic resonance imaging"@en ;
    rdfs:subClassOf KSC_M:00000152 .

nifstd:nlx_inv_090913 rdf:type owl:Class ;
    rdfs:label "diffusion magnetic resonance imaging"@en ;
    nsu:definition "MRI methods that uses strong magnetic fields to visualize the diffusion process of molecules, mainly water, in biological tissues, The diffusion of magnetically oriented water molecules is influenced by the presence of objects, which interact with the former. Thus, biological structures are visualized by the contrast with the signal given by the spin relaxation of water molecules."@en ;
    nsu:synonym "diffusion magnetic resonance imaging"@en ;
    rdfs:subClassOf obo:ERO_0001235 .

nifstd:nlx_inv_090916 rdf:type owl:Class ;
    rdfs:label "diffusion weighted imaging"@en ;
    nsu:definition "dMRI method that calculates the diffusion rate of water in a location of interest, which is then translated in voxel intensity. Best applied to isotropic volumes, such as the gray matter."@en ;
    nsu:synonym "diffusion weighted magnetic resonance imaging assay"@en ;
    rdfs:subClassOf nifstd:nlx_inv_090913 .

nifstd:nlx_inv_090915 rdf:type owl:Class ;
    rdfs:label "diffusion tensor imaging"@en ;
    nsu:definition "dMRI method that is used to visualize anisotropic volumes, such as myelinated fiber tracts. The water molecules align with the tracts, i.e., the diffusion along the fibers is higher than across them. Variants and refinements include the generalized diffusion imaging, and the q-space imaging."@en ;
    nsu:synonym "diffusion tensor imaging assay"@en ;
    rdfs:subClassOf nifstd:nlx_inv_090913 .

KSC_M:00000157 rdf:type owl:Class ;
    rdfs:label "metals tracing"@en ;
    nsu:definition "Tract tracing methods based on the transport of metals along axons. Examples include cobalt- nickel- and magnesium-tracing."@en ;
    rdfs:subClassOf KSC_M:00000125 .

nifstd:nlx_152219 rdf:type owl:Class ;
    rdfs:label "array tomography"@en ;
    nsu:definition "Method used to visualize tens of proteins and peptides in a reconstructed 3D tissue part, together with electron microscopy. A part of interest is fixated in resin, cut in ultrathin slices that are deposited on a single slide, and simultaneously labeled with fluorescently labeled antibodies. The images of all labeled slices are stacked by a fluorescent microscope. After this, the slide can be eluted, and labeled with a different fluorescent antibody.  Finally, after the immunofluorescent analysis, the slices can be imaged by backscatter electron microscopy, and the resulting images are registered with the immunofluorescence images. "@en ;
    nsu:synonym "array tomography"@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:00000159 rdf:type owl:Class ;
    rdfs:label "nucleic acids transfection in xenopus oocytes"@en ;
    nsu:definition "This method is used for functional characterization of proteins in a controlled environment. The expression of genes of interest is realized by their DNA or RNA microinjection into oocytes. This method is widely used for characterization of ion channels."@en ;
    rdfs:subClassOf KSC_M:00000225 .

KSC_M:00000160 rdf:type owl:Class ;
    rdfs:label "single cell multiplex rt-pcr"@en ;
    nsu:definition "Multiplex RT-PCR method applied to an individual neuron, which allows identification and semiquantitative measurement of large sets of expressed proteins and peptides mRNAs. It can be combined with whole-cell patch clamp, and with intracellular filling with dyes."@en ;
    rdfs:subClassOf KSC_M:0000056 .

KSC_M:00000161 rdf:type owl:Class ;
    rdfs:label "anatomical part removal"@en ;
    nsu:definition "A surgery method that separates physically (extracts) an organ or a structural part from the body."@en ;
    rdfs:subClassOf obo:ERO_0000378 .

obo:ERO_0000490 rdf:type owl:Class ;
    rdfs:label "tissue embedding"@en ;
    nsu:definition "Class of methods where an organ or a part of it is embedded in specific materials for further processing and long term preservation."@en ;
    nsu:synonym "tissue embedding"@en ;
    rdfs:subClassOf KSC_M:000002 .

obo:ERO_0000677 rdf:type owl:Class ;
    rdfs:label "tissue fixation"@en ;
    nsu:definition "Class of methods that remove the water from removed organs or parts of it for further processing and long term preservation."@en ;
    nsu:synonym "tissue fixation"@en ;
    rdfs:subClassOf KSC_M:000002 .

KSC_M:00000164 rdf:type owl:Class ;
    rdfs:label "tissue cutting"@en ;
    nsu:definition "Removal of a part of an organ."@en ;
    rdfs:subClassOf KSC_M:00000161 .

obo:ERO_0000491 rdf:type owl:Class ;
    rdfs:label "tissue slicing"@en ;
    nsu:definition "Removal of a part of an organ, or a tissue in identical part (slices) using a specific mechanical device. The most used cutting devices are vibratomes, microtomes, cryotomes, and ultratomes. The thickness of a slice is usually related to the type of experiment or measurement."@en ;
    nsu:synonym "tissue sectioning"@en ;
    rdfs:subClassOf KSC_M:00000164 .

obo:OBI_0600022 rdf:type owl:Class ;
    rdfs:label "excision"@en ;
    nsu:definition "Removal of a part of the gray matter in a mode designed by the experimenter. Used to separate gross divisions of the central nervous system, e.g. the hypothalamus."@en ;
    nsu:synonym "excision"@en ;
    rdfs:subClassOf KSC_M:00000164 .

KSC_M:00000167 rdf:type owl:Class ;
    rdfs:label "transversal slicing"@en ;
    nsu:definition "Removal of a part of the gray matter along the vertical plane of the brain and spinal cord. The result is a set of slices transversal on the longitudinal plane. Also known as frontal or coronal sectioning."@en ;
    nsu:synonym "coronal slicing"@en ;
    nsu:synonym "frontal slicing"@en ;
    nsu:synonym "transversal cutting"@en ;
    rdfs:subClassOf obo:ERO_0000491 .

KSC_M:00000168 rdf:type owl:Class ;
    rdfs:label "longitudinal slicing"@en ;
    nsu:definition "Removal of a part of the gray matter along the horizontal plane of the brain and spinal cord. The result is a set of slices transversal on the vertical or coronal plane. Also known as horizontal sectioning or cutting."@en ;
    nsu:synonym "horizontal cutting"@en ;
    nsu:synonym "horizontal slicing"@en ;
    rdfs:subClassOf obo:ERO_0000491 .

KSC_M:00000169 rdf:type owl:Class ;
    rdfs:label "saggital slicing"@en ;
    nsu:definition "Removal of a part of the gray matter across the horizontal plane of the brain and spinal cord, lateral to medial."@en ;
    rdfs:subClassOf obo:ERO_0000491 .

KSC_M:00000170 rdf:type owl:Class ;
    rdfs:label "oblique slicing"@en ;
    nsu:definition "Removal of a part of the gray matter in a plane oblique to any of the three standard ones."@en ;
    rdfs:subClassOf obo:ERO_0000491 .

:sao819188787 rdf:type owl:Class ;
    rdfs:label "acute slicing"@en ;
    nsu:definition "Cutting method specific to electrophysiological measurements and experiments where the fresh brain is kept and cut in a \"protective\" solution of artificial cerebrospinal fluid."@en ;
    nsu:synonym "acute slice"@en ;
    rdfs:subClassOf obo:ERO_0000491 .

KSC_M:00000172 rdf:type owl:Class ;
    rdfs:label "slice mounting"@en ;
    nsu:definition "Method used for observation, measurement and long term preservation of brain and spinal cord slices where they are put on a support medium (glass slides or film) and covered by a transparent medium (coverslip)."@en ;
    rdfs:subClassOf KSC_M:000002 .

obo:OBI_0302899 rdf:type owl:Class ;
    rdfs:label "decapitation"@en ;
    nsu:definition "Separation of the head from the body, for subsequent extraction of the brain."@en ;
    nsu:synonym "decapitation"@en ;
    rdfs:subClassOf KSC_M:00000161 .

obo:ERO_0001360 rdf:type owl:Class ;
    rdfs:label "stereology"@en ;
    nsu:definition "Class of methods that are used to measure properties of objects usually from lower dimensional samples. A stereological method involves a sampling method, and, depending on the nature of objects and their properties, a measurement method. Stereology is the three-dimensional interpretation of two-dimensional cross sections of materials or tissues. It extracts quantitative information about a three-dimensional material from measurements made on two-dimensional planar sections of the material. Stereology utilizes random, systematic sampling to provide unbiased and quantitative data."@en ;
    nsu:synonym "stereology"@en ;
    rdfs:subClassOf obo:ERO_0001319 ;
    rdfs:subClassOf KSC_M:000006 .

nifstd:nlx_152214 rdf:type owl:Class ;
    rdfs:label "design based stereology"@en ;
    nsu:definition "Class of stereological methods that are independent of geometrical features and spatial distributions of objects. Design based stereological methods are created a priori of the measured objects and their properties."@en ;
    nsu:synonym "design-based stereology"@en ;
    rdfs:subClassOf obo:ERO_0001360 .

nifstd:nlx_152215 rdf:type owl:Class ;
    rdfs:label "model based stereology"@en ;
    nsu:definition "Class of stereological methods based on models partially made on a priori knowledge, and partially based on relationships and rules inferred from previous measurements. These methods are likely to hide biased errors."@en ;
    nsu:synonym "model-based stereology"@en ;
    rdfs:subClassOf obo:ERO_0001360 .

KSC_M:00000178 rdf:type owl:Class ;
    rdfs:label "oblique sectioning"@en ;
    nsu:definition "Removal of a part of the gray matter across a lane of the brain and spinal cord that makes a nonzero angle with any of the three standard (stereotaxic) planes of cutting."@en ;
    rdfs:subClassOf nifstd:nlx_152215 .

KSC_M:00000179 rdf:type owl:Class ;
    rdfs:label "abercrombie method"@en ;
    nsu:definition "Model based stereology method that is used to calculate the average number of cells (points) in a section as the ratio of the product between the number of counted cells and thickness of cells and the sum of the length of cell (in micrometers) and the thickness of a cell. "@en ;
    rdfs:subClassOf nifstd:nlx_152215 .

KSC_M:00000180 rdf:type owl:Class ;
    rdfs:label "dellese principle"@en ;
    nsu:definition "Model based stereology method where the percentage of the volume of the substance is equal with the percentage of area occupied by the substance in plane sections for a homogeneous object."@en ;
    rdfs:subClassOf nifstd:nlx_152215 .

KSC_M:00000181 rdf:type owl:Class ;
    rdfs:label "linear analysis"@en ;
    nsu:definition "Model based stereology method where a grid of equally-spaced parallel lines is superimposed on the plane section. The fraction of total length of these grid lines which lie above the phase of interest, is then an approximation to the area fraction."@en ;
    rdfs:subClassOf nifstd:nlx_152215 .

KSC_M:00000182 rdf:type owl:Class ;
    rdfs:label "glagolev-thomson point-counting method"@en ;
    nsu:definition "Model based stereology method where a grid of equally spaced points is superimposed over the section; the number of points that cover the region of interest is an approximation of its area."@en ;
    rdfs:subClassOf nifstd:nlx_152215 .

KSC_M:00000183 rdf:type owl:Class ;
    rdfs:label "gray level index method"@en ;
    nsu:definition "Design based stereology method proposed by Ziller to approximate the boundaries of a region, based on the gray level index coefficient. It can be addapted to other kinds of tissue classical staining methods."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000184 rdf:type owl:Class ;
    rdfs:label "cavallieri principle"@en ;
    nsu:definition "Method of volumes approximation. The volume of any object (brain region of interest) can be estimated without bias from the profile areas of the cut sections of the BROI. The cuts through the BROI have to systematically-random sampled. The volume is approximated as the product of the BROI's sum of the profile areas on all sections with the distance between the sections."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000185 rdf:type owl:Class ;
    rdfs:label "unbiased virtual counting spaces"@en ;
    nsu:definition "Design-based stereological method used to count neurons in a given volume of tissue using the unbiased virtual counting spaces (optical disector, counting box), consisting of the virtual space within the section thickness and an unbiased counting frame."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000186 rdf:type owl:Class ;
    rdfs:label "physical disector method"@en ;
    nsu:definition "Design-based stereological method used to count neurons in a given volume of tissue using a disector, and based on the analysis of two focal planes within a thick section (the thickness is taken into account, hence the section is 3D), or of two thin (the thickness of sections is negligible, hence the section is 2D] adjacent sections."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000187 rdf:type owl:Class ;
    rdfs:label "fractionator method"@en ;
    nsu:definition "Design-based stereological method used to count neurons in a brain region of interest where the total number of neurons is product of the number of neurons counted within all unbiased virtual counting spaces with the reciprocal value of the sampling probability."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000188 rdf:type owl:Class ;
    rdfs:label "nuclear method"@en ;
    nsu:definition "Design-based stereological method used to estimate mean nuclear volumes of neurons. The midpoint of the nucleolus is selected as sampling unit. The distance from the midpoint of the nucleolus to the nuclear boundary is measured in four mutually orthogonal directions. From the average of the third powers of these measurements an unbiased estimate of the nuclear volume can be obtained. (Gunderson 1988)."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000189 rdf:type owl:Class ;
    rdfs:label "rotator method"@en ;
    nsu:definition "Design-based stereological method used to estimate mean nuclear volumes of neurons where the midpoint of the nucleolus is selected as sampling unit."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000190 rdf:type owl:Class ;
    rdfs:label "point sampled intercepts method"@en ;
    nsu:definition "Design-based stereological method used to estimate mean nuclear volumes of neurons. In this method, all nuclei of neurons that are found in a focal plane are considered. A set of parallel lines is randomly drawn in the focal plane, and the volume a nucleolus is estimated from the lenghts of the intercept of the test line between its nuclear boundaries. [Gunderson, 1988]"@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000191 rdf:type owl:Class ;
    rdfs:label "fakir probes"@en ;
    nsu:definition "Design-based stereological method used for evaluation of surface areas, which is based on counting intersections between the surface and a spatial grid consisting of systematic probes of parallel test lines."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000192 rdf:type owl:Class ;
    rdfs:label "isotropic fakir probes"@en ;
    nsu:definition "Design-based stereological method used for evaluation of surface areas of thick sections (the thickness of the section is non-negligible, therefore the section is a 3D object]. A set of orthogonal triplets of perpendicular test lines are systematically and randomly placed within thick sections containing the surface of interest. The intersections between the surface and the test lines are counted, and the surface area is calculated from the total number of intersections."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000193 rdf:type owl:Class ;
    rdfs:label "space balls method"@en ;
    nsu:definition "Design-based stereological method used for evaluation of the length of fibers. Spheres of hemispheres are systematically and randomly placed in the brain region of interest.  The average density of the linear structure (fiber) is obtained from the total number of intersections between the spheres and the fiber. The length of the fiber is estimated as the product between the length density and the estimated volume of the brain region of interest."@en ;
    rdfs:subClassOf obo:ERO_0001360 .

KSC_M:00000194 rdf:type owl:Class ;
    rdfs:label "isotropic virtual planes"@en ;
    nsu:definition "Design-based stereological method used for evaluation of the length of fibers in thick samples (the thickness is non-negligible, hence the object is 3D). The length of the fiber is evaluated using software-randomized isotropic virtual planes in volume probes, in systematically sampled microscope fields."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000195 rdf:type owl:Class ;
    rdfs:label "proportionator method"@en ;
    nsu:definition "Design-based stereological method used for evaluation of the number of neurons in a given tissue that is similar to the fractionator method, but the sampling procedure is dependent of the neurons sizes."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000196 rdf:type owl:Class ;
    rdfs:label "nearest neighbor analysis"@en ;
    nsu:definition "Design-based stereological method used for evaluation of the cytoarchitecture of a brain region of interest. The method is similar to the nearest-neighbor distance distribution function analysis. This method requires thick sections."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000197 rdf:type owl:Class ;
    rdfs:label "voronoi tesselation"@en ;
    nsu:definition "Design-based stereological method used for evaluation of the cytoarchitecture of a brain region of interest. The method first computes the vicinity of a neuron, i.e., a polygon around the neuron, the Voronoi or Dirichlet polygon. This is repeated for each neuron in such way that each of them is included in a polygon, or tile (tesselation), or cell."@en ;
    rdfs:subClassOf nifstd:nlx_152214 .

KSC_M:00000198 rdf:type owl:Class ;
    rdfs:label "voltage clamp"@en ;
    nsu:definition "Class of intracellular electrophysiological techniques where the trans-membrane difference of potential (voltage) is kept constant, and the ion currents are measured."@en ;
    rdfs:subClassOf nifstd:nlx_inv_090910 .

KSC_M:00000199 rdf:type owl:Class ;
    rdfs:label "two-electrode voltage clamp"@en ;
    nsu:definition "Voltage clamp method that uses two intracellular electrodes instead of one, and is preferred for measuring high currents and for larger cells such as oocytes."@en ;
    rdfs:subClassOf KSC_M:00000198 .

KSC_M:00000200 rdf:type owl:Class ;
    rdfs:label "dual cell voltage clamp"@en ;
    nsu:definition "Variation of the two-electrode voltage clamp method specifically designed to study kinetics and conductance of gap junctions."@en ;
    rdfs:subClassOf KSC_M:00000198 .

KSC_M:00000201 rdf:type owl:Class ;
    rdfs:label "single electrode voltage clamp"@en ;
    nsu:definition "Class of voltage clamp methods where a single intracellular electrod is used for both clamping the cell and for measurement. Used for small cells, or cells that are difficult to get to. Also used for study of plants cells."@en ;
    rdfs:subClassOf KSC_M:00000198 .

KSC_M:00000202 rdf:type owl:Class ;
    rdfs:label "continuous single electrode voltage clamp method"@en ;
    nsu:definition "Single electrode voltage clamp method where both measurement and clamping are performed in the same time."@en ;
    rdfs:subClassOf KSC_M:00000201 .

KSC_M:00000203 rdf:type owl:Class ;
    rdfs:label "discontinuous single electrode voltage clamp method"@en ;
    nsu:definition "Single electrode voltage clamp method where the sharp electrode is rapid switched between the measurement mode and the clamping mode. The cell membrane needs to be enzymatically removed."@en ;
    rdfs:subClassOf KSC_M:00000201 .

KSC_M:00000204 rdf:type owl:Class ;
    rdfs:label "current clamp"@en ;
    nsu:definition "Intracellular electrophysiological technique where the current is kept fixed by the experimenter and the voltage is measured."@en ;
    rdfs:subClassOf nifstd:nlx_inv_090910 .

KSC_M:00000205 rdf:type owl:Class ;
    rdfs:label "amperometry"@en ;
    nsu:definition "Extracellular method used for detection of ions in an aqueous solution, based on electric current or changes in electric current. Also known as pulsed amperometric detection."@en ;
    rdfs:subClassOf nifstd:birnlex_2272 .

KSC_M:00000206 rdf:type owl:Class ;
    rdfs:label "single potential amperometry"@en ;
    nsu:definition "Amperometry method where a direct current is applied into the solution. It is used to detect small currents provided by weak ions. Also known as chronoamperometry."@en ;
    rdfs:subClassOf KSC_M:00000205 .

KSC_M:00000207 rdf:type owl:Class ;
    rdfs:label "pulsed amperometry"@en ;
    nsu:definition "Amperometry method where a working voltage is applied for a short period of time, followed by different potentials designed to clean the electrodes."@en ;
    rdfs:subClassOf KSC_M:00000205 .

KSC_M:00000208 rdf:type owl:Class ;
    rdfs:label "voltammetry"@en ;
    nsu:definition "Extracellular electrophysiological method designed to measure currents when a continuous or variable differential of potential is applied. A small and linearly varying potential is applied between a microelectrode and the medium and the resulting current-potential curve (voltammogram) is recorded. The voltammogram is characteristic of the nature and concentration of electro-oxidizable or reducible materials close to the electrode surface. The cycle can be repetead for smaller or higher values of the applied voltage."@en ;
    rdfs:subClassOf nifstd:birnlex_2272 .

nifstd:birnlex_2293 rdf:type owl:Class ;
    rdfs:label "electroencephalography"@en ;
    nsu:definition "Electrophysiological method designed to measure the electrical activity of the brain at the level of the scalp."@en ;
    nsu:synonym "electroencephalography recording protocol"@en ;
    rdfs:subClassOf nifstd:birnlex_2272 .

KSC_M:00000210 rdf:type owl:Class ;
    rdfs:label "electrocortigraphy"@en ;
    nsu:definition "Also known as intracranial EEG (iEEG), it is an invasive electrophysiological method designed to measure electrical activity at the level of the surface of cortex. The electrodes are placed directly onto the surface of the cerebral cortex."@en ;
    rdfs:subClassOf nifstd:birnlex_2272 .

KSC_M:00000211 rdf:type owl:Class ;
    rdfs:label "simple random sampling"@en ;
    nsu:definition "Sampling statistical method where all any member of the population is considered to have the same probability of selection. The selection of units is performed by using previously defined randomization procedures."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000212 rdf:type owl:Class ;
    rdfs:label "stratified random sampling"@en ;
    nsu:definition "Sampling statistical method where the units of the population are distributed over a number of non-overlaping \"strata' (the population is partitioned). The members of the population will have the values of the variable used for partitioning much closer one to the other than any unit from a different stratum. The sampling procedure is applied to each stratum. Two most commonly used criteria to divide a population in strata are the spatial location, and sizes of units."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000213 rdf:type owl:Class ;
    rdfs:label "systematic sampling"@en ;
    nsu:definition "Sampling method that is based on ordering of the population according to a sampling frame. The method usually employs the equi-probability approach where process of choosing the sample units is circular. First, an element is sampled at random, and from here each unit that is a specific distance from the previous (the skip) will be sampled. When this process gets to the end of the list, it restarts from the beginning. This method is applied when the population is logically homogeneous, and only when the population does not has any hidden pattern."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000214 rdf:type owl:Class ;
    rdfs:label "cluster sampling"@en ;
    nsu:definition "Sampling method where the units are grouped together according to some criteria (clusters) are randomly sampled together and then all measured."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000215 rdf:type owl:Class ;
    rdfs:label "multistage sampling"@en ;
    nsu:definition "Sampling method where the units are hierarchically organized, and the units are randomly sampled at each level."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000216 rdf:type owl:Class ;
    rdfs:label "unequal probability sampling"@en ;
    nsu:definition "Sampling method where the probability of a unit being sampled is a function of the characteristics of that unit."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000217 rdf:type owl:Class ;
    rdfs:label "adaptive cluster sampling"@en ;
    nsu:definition "Sampling method that is similar with cluster sampling, but the sizes (the number of units) of clusters and their number is unknown. In this method, a prior value C is established. If a given unit is higher or equal than C, then the units in its neighborhood are sampled."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000218 rdf:type owl:Class ;
    rdfs:label "line transect sampling"@en ;
    nsu:definition "Sampling method that is used for objects that are difficult to observe, or are rare. In this method, the observer moves along a line acroos the considered area looking at the right and left and noting each occurence of a given object. Line transects are walked, flown, or traversed, and the perpendicular distances to all detected objects of interest are recorded. Also known as distance sampling."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000219 rdf:type owl:Class ;
    rdfs:label "composite sampling"@en ;
    nsu:definition "Sampling methods designed for extracting subsamples from initial samples. First, the sampled units are grouped in composite units, and these will be further sampled."@en ;
    rdfs:subClassOf KSC_M:00000113 .

KSC_M:00000220 rdf:type owl:Class ;
    rdfs:label "clarity"@en ;
    nsu:definition "Neuronal tissue vizualization method where the lipids from the membranes of neurons are eliminated through polymerization and electrophoresis. After this procedure is applied the tissue become translucid, and it can be investigated with sheet microscopy. The tissue preserves the initial form."@en ;
    rdfs:subClassOf KSC_M:000004 .

KSC_M:00000221 rdf:type owl:Class ;
    rdfs:label "infusion"@en ;
    nsu:definition "Class of methods of administration of substances into the body. Also known as route of administration (medicine)."@en ;
    nsu:synonym "route of administration"@en ;
    rdfs:subClassOf :nsm_0000000 .

nifstd:birnlex_2135 rdf:type owl:Class ;
    rdfs:label "injection"@en ;
    nsu:definition "Class of infusion methods that use specific devices for administration of substances directly into the body. These devices (syringes with hollow needles, or pipettes) penetrate the skin of the body, and the substance is delivered mechanically, or using electric currents for micropipettes. Injections can be subdivided by the specific part of the body where the substance needs to be administrated, by the specific device of delivery, and by the mechanism of delivery."@en ;
    nsu:synonym "injection"@en ;
    rdfs:subClassOf KSC_M:00000221 .

KSC_M:00000223 rdf:type owl:Class ;
    rdfs:label "pressure injection"@en ;
    nsu:definition "Injection method where a substance is delivered mechanically via a syringe or a pipette into the tissue of interest."@en ;
    rdfs:subClassOf nifstd:birnlex_2135 .

KSC_M:00000224 rdf:type owl:Class ;
    rdfs:label "iontophoretic injection"@en ;
    nsu:definition "Injection method where a substance is delivered in the tissue of interest by applying a small and temporary electrical current to the solution in the pipette."@en ;
    rdfs:subClassOf nifstd:birnlex_2135 .

KSC_M:00000225 rdf:type owl:Class ;
    rdfs:label "nucleic acids and proteins measurement"@en ;
    nsu:definition "Class of methods used for identification and measurement of nucleic acids and their products."@en ;
    rdfs:subClassOf :nsm_0000000 .

KSC_M:00000226 rdf:type owl:Class ;
    rdfs:label "fast blue tracing"@en ;
    nsu:definition "Retrograde fluorescent tract tracing method."@en ;
    rdfs:subClassOf KSC_M:00000129 .

KSC_M:00000227 rdf:type owl:Class ;
    rdfs:label "evans blue tracing"@en ;
    nsu:definition "Retrograde fluorescent tract tracing method."@en ;
    rdfs:subClassOf KSC_M:00000129 .

KSC_M:00000228 rdf:type owl:Class ;
    rdfs:label "diamidino yellow tracing"@en ;
    nsu:definition "Retrograde fluorescent tract tracing method."@en ;
    rdfs:subClassOf KSC_M:00000129 .

KSC_M:00000229 rdf:type owl:Class ;
    rdfs:label "fluorogold tracing"@en ;
    nsu:definition "Retrograde fluorescent tract tracing method."@en ;
    rdfs:subClassOf KSC_M:00000129 .

obo:ERO_0000201 rdf:type owl:Class ;
    rdfs:label "microscopy"@en ;
    nsu:definition "Class of methods for visualization of objects that use a specific device, the microscope."@en ;
    nsu:synonym "microscopy"@en ;
    rdfs:subClassOf KSC_M:000004 .

